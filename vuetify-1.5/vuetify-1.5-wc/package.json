{
  "name": "vuetify-1.5-wc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:app": "vue-cli-service build --target app",
    "serve:app": "vue-cli-service serve",
    "build:wc": "vue-cli-service build --target wc --name custom-component './src/components/*.vue'",
    "serve:wc": "serve -l 8888 dist/",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "css-loader": "^3.2.0",
    "material-design-icons-iconfont": "^5.0.1",
    "vue": "^2.6.10",
    "vuetify": "^1.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "serve": "^11.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
